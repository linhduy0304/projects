/**
 * # reducers
 * 
 * This class combines all the reducers into one
 * 
 */
'use strict';
/**
 * ## Imports
 * 
 * our 4 reducers
 */ 
import auth from './auth/reducer';

import { combineReducers } from 'redux';

const rootReducer = combineReducers({
  auth,
});

export default rootReducer;